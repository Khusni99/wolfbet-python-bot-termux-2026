              "result_value": {
                "description": "Result value",
                "type": "string",
                "example": "1.65"
              },
              "state": {
                "description": "State of bet",
                "type": "string",
                "enum": [
                  "loss",
                  "win"
                ]
              },
              "published_at": {
                "description": "Published at timestamp",
                "type": "integer",
                "example": 1582549252
              },
              "game": {
                "description": "Game details",
                "type": "object",
                "properties": {
                  "id": {
                    "description": "Game id",
                    "type": "integer",
                    "example": 1
                  },
                  "name": {
                    "description": "Game name",
                    "type": "string",
                    "enum": [
                      "dice"
                    ]
