    "schemas": {
      "BetRequest": {
        "type": "object",
        "properties": {
          "currency": {
            "description": "Currency",
            "type": "string",
            "enum": [
              "btc",
              "eth",
              "ltc",
              "doge",
              "trx",
              "bch",
              "xrp",
              "usdt",
              "uni",
              "sushi",
              "xlm",
              "etc",
              "bnb",
              "dot",
              "ada",
              "shib",
              "matic",
              "optim"
            ]
          },
          "game": {
            "description": "Game",
            "type": "string",
            "enum": [
              "dice"
            ]
          },
          "amount": {
            "description": "Amount",
            "type": "number",
            "example": "0.002"
          },
          "rule": {
            "description": "Rule",
            "type": "string",
            "enum": [
              "over",
              "under"
            ]
          },
          "multiplier": {
            "description": "Multiplier",
            "type": "number",
            "example": "1.98"
          },
          "bet_value": {
            "description": "Bet value",
            "type": "number",
            "example": "49.99"
          }
        }
      },
      "BetResponse": {
        "type": "object",
