          "bet": {
            "description": "Bet details",
            "type": "object",
            "properties": {
              "hash": {
                "description": "Hash",
                "type": "string",
                "minLength": 36,
                "maxLength": 36,
                "example": "3deb5a81-d914-445a-9820-fdd655430bde"
              },
              "nonce": {
                "description": "Nonce",
                "type": "integer",
                "example": 1
              },
              "currency": {
                "description": "Currency",
                "type": "string",
                "enum": [
                  "btc",
                  "eth",
                  "ltc",
                  "doge",
                  "trx",
                  "bch",
                  "xrp"
                ]
              },
              "amount": {
                "description": "Amount",
                "type": "string",
                "example": "0.0001"
